@model IModelRepository


<!-- Container (About Section) -->
<div class="w3-content w3-container w3-padding-64" id="aboutUs">
    <h3 class="w3-center">About Us</h3>
    <p>Tech Entrepreneurship is a first and foremost a social club. We
    want to provide students with the opportunity to meet others who share
    their interest in starting businesses. We also like to learn things, 
    and to share our knowledge with other students.</p>
</div>

<div class="w3-row w3-center w3-dark-grey w3-padding-16" id="Events">
</div>

<!-- Second Parallax Image with Events Text -->
<div class="bgimg-3 w3-display-container w3-opacity-min">
    <div class="w3-display-middle">
        <span class="w3-xxlarge w3-text-white w3-wide wow bounceInDown">GROUP EVENTS</span>
    </div>
</div>
<div class="w3-row w3-center w3-dark-grey w3-padding-16">
</div>

@await Component.InvokeAsync("Event", Model)

<div class="w3-row w3-center w3-dark-grey w3-padding-16" id="Books">
</div>

<!-- Third Parallax Image with Books Text -->
<div class="bgimg-2 w3-display-container w3-opacity-min">
    <div class="w3-display-middle">
        <span class="w3-xxlarge w3-text-white w3-wide wow bounceInDown">BOOK CLUB</span>
    </div>
</div>

@await Component.InvokeAsync("Book", Model)  

<div class="w3-row w3-center w3-dark-grey w3-padding-16">
</div>













